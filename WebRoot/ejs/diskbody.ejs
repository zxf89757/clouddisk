	    <%for(var i=0;i<data.folderList.length;i++){var folder=data.folderList[i];%>  
	         <tr>
				<td>
					<span class="glyphicon glyphicon-folder-open"></span>
					<a onclick="getContents(<%=folder.folderid%>)"><%=folder.foldername%></a>
				</td>
				<td>-</td>
				<td><%=folder.folderdata%></td>
				<td>
					<a href="javascript:void(0);" onclick="deletefolder(<%=folder.folderid%>)"/>删除</a>
					<% if (folder.islock==0) { %>
						<a href="javascript:void(0);" onclick="updatefolder(<%=folder.folderid%>,1)"/>锁定</a>
					<% } %>
					<% if (folder.islock!=0) { %>
						<a href="javascript:void(0);" onclick="updatefolder(<%=folder.folderid%>,0)"/>解锁</a>
					<% } %>
				</td>
			</tr> 
	    <%}%>
	    <%for(var i=0;i<data.resList.length;i++){var resourse=data.resList[i];%>  
	         <tr>
				<td>
					<span class="glyphicon glyphicon-file"></span>
					<%=resourse.userresname%>
				</td>
				<td><%=resourse.res.size%>M</td>
				<td><%=resourse.userresupdatetime%></td>
				<td>
					<a href="javascript:void(0);" onclick="deleteuserres(<%=resourse.parentid%>,<%=resourse.res.id%>)"/>删除</a>
					<% if (resourse.islock==0) { %>
						<a href="javascript:void(0);" onclick="updateuserres(<%=resourse.parentid%>,<%=resourse.res.id%>,1)"/>锁定</a>
					<% } %>
					<% if (resourse.islock!=0) { %>
						<a href="javascript:void(0);" onclick="updateuserres(<%=resourse.parentid%>,<%=resourse.res.id%>,0)"/>解锁</a>
					<% } %>
					<a href="/clouddisk/userres/<%=resourse.parentid%>/download/<%=resourse.res.id%>">下载</a>
				</td>
			</tr> 
	    <%}%>